<mvvm:TinyApplication x:Class="Wirtualnik.Maui.App"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:Wirtualnik.Maui"
    xmlns:mvvm="clr-namespace:TinyMvvm;assembly=TinyMvvm.Maui"
    xmlns:templates="clr-namespace:Wirtualnik.Maui.Controls.Templates">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Resources/Values/Colors.xaml" />
                <ResourceDictionary Source="Resources/Values/FontIcons.xaml" />
            </ResourceDictionary.MergedDictionaries>

            <templates:ShellTabbedPageTemplate x:Key="ShellTabbedPageTemplate" />
        </ResourceDictionary>
    </Application.Resources>
</mvvm:TinyApplication>
